{
    "name": "xoesae/valid8",
    "description": "A lightweight php validator",
    "keywords": ["validator", "ligth", "valid"],
    "type": "library",
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Xoesae\\Valid8\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xoesae\\Valid8\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    }
}
